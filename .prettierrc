{
  "arrowParens": "always",
  "trailingComma": "all",
  "tabWidth": 2,
  "semi": true,
  "singleQuote": true,
  "importOrder": [
    "react",
    "<THIRD_PARTY_MODULES>",
    "^components/(.*)$",
    "^hooks/(.*)$",
    "^services/(.*)$",
    "^util/(.*)$",
    "^[./]",
    "/.*.scss$/"
  ],
  "importOrderSeparation": true,
  "importOrderSortSpecifiers": true
}